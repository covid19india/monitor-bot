        function face(url) {
            var id = url.id.split('^');
            var Window = window.open(
                "http://www.facebook.com/sharer.php?u=" + id[0] + "&amp;t" + id[1] + "",
                "_blank", "width=400, height=450");
        }
        function twit(url) {
            var id = url.id.split('^');
            var Window = window.open(
                "http://twitter.com/share?text=" + id[1] + "&amp;url" + id[0] + "",
                "_blank", "width=400, height=450");
        }
