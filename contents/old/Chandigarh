    function googleTranslateElementInit() {
      new google.translate.TranslateElement({pageLanguage: 'en', includedLanguages: 'hi,pa', layout: google.translate.TranslateElement.InlineLayout.SIMPLE}, 'google_translate_element');
    } 
    
	$(window).on( 'load', function() {

      if( $(window).width() < 768 )  {
        var str = $(location).attr('pathname');
        var res = str.split("/");
        if(res[res.length-1]=='volunteer'){
            
        }else if(res[res.length-1]=='grocery'){
            
        }else if(res[res.length-1]=='cart'){
            
        }else{
         $('#appDownloadModal').modal('show');
        }
      }
    
    });

    $( function() {  
      
	  $('.fancybox').fancybox();
                
      $('.event-img3 .fancybox').mouseover(function() {
          if($(this).has('span')){
              $(this).find('span').show();
          }    
        }).mouseout(function() {
              $(this).find('span').hide();
      });

  
      $( "#accordion" ).accordion();
      
      $(".skip-link").click(function() {
          $('html, body').animate({
              scrollTop: $(".skip-content").offset().top
          }, 2000);
      });

      var originalSizep = $('#about p').css('font-size');    
      var originalSizea = $('#about a').css('font-size');    
      var originalSizefp = $('footer p').css('font-size');    
      var originalSizefa = $('footer a').css('font-size');    
      //var originalSizefp = $('.footer p').css('font-size');    
     
      // reset        
      $(".resetMe").click(function(){           
        $('#about p').css('font-size', originalSizep);         
        $('#about a').css('font-size', originalSizea);         
        $('footer p').css('font-size', originalSizefp);         
        //$('.footer p').css('font-size', originalSizefp);         
        $('footer a').css('font-size', originalSizefa);         
      });
   
      // Increase Font Size          
      $(".increase").click(function(){         
        var currentSizea = $('#about a').css('font-size'); 
        var currentSizea = parseFloat(currentSizea)*1.2;          
        $('#about a').css('font-size', currentSizea);

        var currentSizep = $('#about p').css('font-size');         
        var currentSizep = parseFloat(currentSizep)*1.2;          
        $('#about p').css('font-size', currentSizep);         
        
        var currentSizefp = $('footer p').css('font-size');         
        var currentSizefp = parseFloat(currentSizefp)*1.2;          
        $('footer p').css('font-size', currentSizefp);         
        
        var currentSizefa = $('footer a').css('font-size');         
        var currentSizefa = parseFloat(currentSizefa)*1.2;          
        $('footer a').css('font-size', currentSizefa);         
        
        return false;          
      
      });        

      // Decrease Font Size       
      $(".decrease").click(function(){

        var currentSizea = $('#about a').css('font-size');        
        var currentSizea = parseFloat(currentSizea)*0.8;        
        $('#about a').css('font-size', currentSizea); 

        var currentSizep = $('#about p').css('font-size');        
        var currentSizep = parseFloat(currentSizep)*0.8;        
        $('#about p').css('font-size', currentSizep); 
        
        var currentSizefp = $('footer p').css('font-size');        
        var currentSizefp = parseFloat(currentSizefp)*0.8;        
        $('footer p').css('font-size', currentSizefp); 
        
        var currentSizefa = $('footer a').css('font-size');        
        var currentSizefa = parseFloat(currentSizefa)*0.8;        
        $('footer a').css('font-size', currentSizefa); 
        
        return false;         

      }); 
    
    });

  </script>
<script>
$(document).ready(function(){
 
 $('.add_cart').click(function(){
  var product_id = $(this).data("productid");
  var product_name = $(this).data("productname");
  var product_price = $(this).data("price");
  var quantity = $('#' + product_id).val();
  if(quantity != '' && quantity > 0)
  {
   $.ajax({
    url:"http://chdcovid19.in/index.php/grocery/add",
    method:"POST",
    data:{product_id:product_id, product_name:product_name, product_price:product_price, quantity:quantity},
    success:function(data)
    {
     alert("Product Added into Cart");
     $('#cart_details').html(data);
     $('#' + product_id).val('');
    }
   });
  }
  else
  {
   alert("Please Enter quantity");
  }
 });

 //$('#cart_details').load("http://chdcovid19.in/index.php/grocery/load");

 $(document).on('click', '.remove_inventory', function(){
  var row_id = $(this).attr("id");
  if(confirm("Are you sure you want to remove this?"))
  {
   $.ajax({
    url:"http://chdcovid19.in/index.php/grocery/remove",
    method:"POST",
    data:{row_id:row_id},
    success:function(data)
    {
     alert("Product removed from Cart");
     $('#cart_details').html(data);
    }
   });
  }
  else
  {
   return false;
  }
 });

 $(document).on('click', '#clear_cart', function(){
  if(confirm("Are you sure you want to clear cart?"))
  {
   $.ajax({
    url:"http://chdcovid19.in/index.php/grocery/clear",
    success:function(data)
    {
     alert("Your cart has been clear...");
     $('#cart_details').html(data);
    }
   });
  }
  else
  {
   return false;
  }
 });

 $(document).on('change', '#location', function(){
   var location = $(this).val();
   $.ajax({
    url:"http://chdcovid19.in/index.php/grocery/vendor",
     method:"POST",
    data:{location:location},
    success:function(data)
    {
      
     $('#vendor_name').html(data);
    }
   });
 
  
 });
 
 $(document).on('change', '.vtype', function(){
   var type = $(this).val();
    if(type=='individual'){
         window.location = 'http://chdcovid19.in/index.php/volunteer'; // redirect
    }else{
        window.location = 'http://chdcovid19.in/index.php/volunteer/organization';
    }
    
    return false;
  
 });
    
