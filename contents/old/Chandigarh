     createRecovered();
    createActive();
    
    function createConfirmed(){
    	var dps = [];
    	var yValue, yTotal = 0, yPercent = 0;
    	for(var i = 0; i < cdata.length; i++){
    		yValue = cdata[i].y;
    		yPercent += yValue;
    		dps.push({label: cdata[i].label, y: yPercent});
    	}
    	chart3.addTo("data",{ name: "Total Cases",type:"spline",markerSize: 0,showInLegend: true, yValueFormatString: "0.##\"\"", dataPoints: dps});
    }
    function createRecovered(){
    	var aps = [];
    	var yValue, yTotal = 0, yPercent = 0;
    	for(var i = 0; i < rdata.length; i++){
    		yValue = rdata[i].y;
    		yPercent += yValue;
    		aps.push({label: rdata[i].label, y: yPercent});
    	}
    	chart3.addTo("data",{ name: "Recovered ",type:"spline",markerSize: 0,showInLegend: true, yValueFormatString: "0.##\"\"", dataPoints: aps});
    }
    function createActive(){
    var dps = [];
    	var yValue, yTotal = 0, yPercent = 0;
    	for(var i = 0; i < cdata.length; i++){
    		yValue = cdata[i].y - rdata[i].y - ddata[i].y;
    		yPercent += yValue;
    		dps.push({label: cdata[i].label, y: yPercent});
    	}
    	chart3.addTo("data",{ name: "Active Cases",type:"spline",markerSize: 0,showInLegend: true, yValueFormatString: "0.##\"\"", dataPoints: dps});
    }
    function createDeath(){
        var aps = [];
    	var yValue, yTotal = 0, yPercent = 0;
    	for(var i = 0; i < ddata.length; i++){
    		yValue = ddata[i].y;
    		yPercent += yValue;
    		aps.push({label: ddata[i].label, y: yPercent});
    	}
    	chart3.addTo("data",{ name: "Death",type:"spline",markerSize: 0,lineThickness: 5,showInLegend: true, yValueFormatString: "0.##\"\"", dataPoints: aps});
    }
}

</script>
</body>
</html>
